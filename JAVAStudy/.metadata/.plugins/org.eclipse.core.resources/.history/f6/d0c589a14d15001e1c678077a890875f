package 월급계산프로그램;

public class Main {

	public static void main(String[] args) {
		RegularEmployee regular = new RegularEmployee();
		regular.Employee("SMHRD001", "홍길동", 4000 ,400);
		System.out.println(regular.print());
		
		TempEmployee temp = new TempEmployee();
		temp.Employee("SMHRD002", "박문수", 3000);
		System.out.println(temp.print());
		
		PartTimeEmployee partTime = new PartTimeEmployee();
		partTime.Employee("SMHRD003", "임성훈", 10, 10);
		System.out.println(partTime.print());

	}

}
