package 게임만들기;

import java.util.Random;

public class PlusGame implements GameRule{

	@Override
	public int random() {
		Random rand = new Random();
		int[] numArr = new int[10];
		for (int i = 0; i < 10; i++) {
			numArr[i] = rand.nextInt(1, 11);
		}
		
		String[] addArr = {"+", "-","*","%"};
		for (int j = 0; j < 5; j++) {
			addArr[j] = rand.toString();
		}
		
		return 0;
	}

	@Override
	public String getQuizMsg() {
//		String[] arr = {"+", "-","*","%"};
//		for (int i = 0; i < 4; i++) {
//		
//		}
		for (int i = 0; i < 5; i++) {
			for (int j = 0; j < 9; j++) {
				System.out.println();
			}
		}
		return null;
	}

	@Override
	public boolean checkAnswer(int ans) {
		
		return false;
	}

}
