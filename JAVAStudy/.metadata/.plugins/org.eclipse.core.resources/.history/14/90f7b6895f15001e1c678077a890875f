package 인터페이스;

public class Pikca implements PokeMon{

	// 피카츄 캐릭터를 사장님이 주신 인터페이스를 통해 완성해보자!
	// 인터페이스를 사용하는 키워드 : implements(구현)
	
	@Override
	public void atk() {
		// TODO Auto-generated method stub
		System.out.println("펀치");
	}

	@Override
	public void specialAtk() {
		// TODO Auto-generated method stub
		System.out.println("백만볼트");
	}
	
}
