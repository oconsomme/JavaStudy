package 월급계산프로그램;

public class Employee {
	// Regular/PartTime/TempEmployee가 
	// 가지고 있는 공통적인 필드, 메서드가 있음
	
	// Regular/PartTime/TempEmployee가 
	// Employee 클래스를 상속받을 예정!!!3
	
	String empno;
	String name;
	int pay;
	
	public int getMoneyPay() {
		return pay;
	}
	public String print() {
		return (empno+" : "+name+" : "+pay+"만원");
	}
	public void setEmployee(String empno, String name, int pay){
		this.empno = empno;
		this.name = name;
		this.pay = pay;
	}
}
