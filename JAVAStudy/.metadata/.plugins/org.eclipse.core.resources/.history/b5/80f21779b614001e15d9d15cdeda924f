package 월급계산프로그램;

public class RegularEmployee extends Employee{
	int bonus;
	
	public void RegularEmployee(String empno, String name, int pay, int bonus) {
		this.empno = empno;
		this.name = name;
		this.pay = pay;
		this.bonus = bonus;
	}
	
	
	public void regular() {
		System.out.println(empno+" : "+name+" : "+pay);
	}
	
}
